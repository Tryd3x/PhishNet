{
    "name": "PhishNet",
    "manifest_version": 2,
    "description": "Extension that detects phishing websites",
    "version": "1.0",
    "icons": {
        "16": "/assets/shield.png",
        "48": "/assets/shield.png",
        "128": "/assets/shield.png"
    },
    "browser_action": {
        "default_popup": "extension.html",
        "default_icon": "/assets/shield.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"],
            "css":[]
        }
    ]
}